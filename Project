Color: #5c4b51;

}

Body{

Background: linear-gradient(to right, #f4b661, #f16160);

}

.wrapper{

Min-height: 100vh;

Display: flex;

Justify-content: center;

Align-items: center;

}

.form{

Width: 425px;

Height: auto;

Background: #fff;

Padding: 35px 50px;

Border-radius: 2px;

}

.form .title{

